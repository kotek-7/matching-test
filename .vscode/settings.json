{
  "eslint.enable": false,
  "prettier.enable": false,
  "oxc.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "never",
    "source.fixAll.eslint": "never"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "editor.defaultFormatter": "biomejs.biome",
  "typescript.preferences.includePackageJsonAutoImports": "auto",
  "files.associations": {
    "wrangler.json": "jsonc"
  }
}
